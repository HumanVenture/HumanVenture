<!DOCTYPE html>
<html>
	<head>
		{% block head %}
			{% include 'head.twig' %}
		{% endblock %}
	</head>
	<body>
		{% block body %}
			<header id="main-header">
				{% block main_header %}
					{% include 'main-header.twig' %}
				{% endblock %}
			</header>

			<section class="content-wrapper" role="main">
				{% block content %}
				{% endblock %}
			</section>

			<div class="social-plugins-wrapper">
				{% block social_plugins %}
				{% endblock %}
			</div>

			<footer id="main-footer">
				{% block main_footer %}
					{% include 'main-footer.twig' %}
				{% endblock %}
			</footer>
		{% endblock %}
	</body>
</html>
